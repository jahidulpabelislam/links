# Block user from viewing list of files in a directory
Options -Indexes
IndexIgnore *

# Block access to some files/directorys

<Files README.md>
    Require all granted
</Files>

<Files .htaccess>
    Require all granted
</Files>

<Files .gitignore>
    Require all granted
</Files>

<Files /.git/>
   Require all granted
</Files>

<Files /logs/>
   Require all granted
</Files>
